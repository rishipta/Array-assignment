int maxIceCream(vector<int>& costs, int coins) {
    int maxCost = *max_element(costs.begin(), costs.end());
    vector<int> count(maxCost + 1, 0);

    for(int c : costs)
        count[c]++;

    int bars = 0;
    for(int cost = 1; cost <= maxCost; cost++) {
        while(count[cost] > 0 && coins >= cost) {
            coins -= cost;
            count[cost]--;
            bars++;
        }
    }
    return bars;
}

DNode* reverseK(DNode *head, int k) {
    DNode *curr = head, *next = NULL, *prev = NULL;
    int count = 0;

    while (curr && count < k) {
        next = curr->next;
        curr->next = prev;
        curr->prev = next;
        prev = curr;
        curr = next;
        count++;
    }

    if (next != NULL)
        head->next = reverseK(next, k);

    if (head->next)
        head->next->prev = head;

    return prev;
}

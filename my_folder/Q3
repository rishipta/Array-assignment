#include <stdio.h>
#include <stdlib.h>

#define SIZE 100

typedef struct {
    int arr[SIZE];
    int front, rear;
} Queue;

void init(Queue *q) {
    q->front = q->rear = -1;
}

int isEmpty(Queue *q) {
    return q->front == -1;
}

void enqueue(Queue *q, int val) {
    if (q->rear == SIZE - 1) return;
    if (q->front == -1) q->front = 0;
    q->arr[++q->rear] = val;
}

int dequeue(Queue *q) {
    if (isEmpty(q)) return -1;
    int val = q->arr[q->front];
    if (q->front == q->rear)
        q->front = q->rear = -1;
    else
        q->front++;
    return val;
}

int main() {
    Queue q1, firstHalf, result;
    init(&q1);
    init(&firstHalf);
    init(&result);

    int n = 6;
    int input[] = {4, 7, 11, 20, 5, 9};

    for (int i = 0; i < n; i++)
        enqueue(&q1, input[i]);

    // Step 1: Move first half to another queue
    for (int i = 0; i < n / 2; i++)
        enqueue(&firstHalf, dequeue(&q1));

    // Step 2: Interleave both halves
    while (!isEmpty(&firstHalf)) {
        enqueue(&result, dequeue(&firstHalf));
        enqueue(&result, dequeue(&q1));
    }

    // Print output
    printf("Interleaved Output: ");
    while (!isEmpty(&result))
        printf("%d ", dequeue(&result));

    return 0;
}
